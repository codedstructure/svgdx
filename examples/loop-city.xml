<svg>
  <loop count="6" loop-var="i" step="20">
    <loop count="6" loop-var="j" step="20">
      <loop count="{{randint(2,6)}}" loop-var="k" start="{{randint(0,10) / 10}}" step="{{-2 * random()}}">
        <rect wh="15" xy="{{$j + $k, $i + $k}}" class="d-softshadow"/>
      </loop>
    </loop>
  </loop>
</svg>
